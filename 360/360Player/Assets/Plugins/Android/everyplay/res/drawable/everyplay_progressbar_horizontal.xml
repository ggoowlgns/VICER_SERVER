<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >
  <item android:id="@android:id/background">
    <shape>
      <gradient
        android:angle="270"
        android:centerColor="#00122f83"
        android:centerY="0.5"
        android:endColor="#00122f83"
        android:startColor="#00122f83" />
    </shape>
  </item>
  <item android:id="@android:id/progress">
    <clip>
      <shape>
        <gradient
          android:angle="0"
          android:endColor="#ff85d2ff"
          android:startColor="#ff00A0FF" />
      </shape>
    </clip>
  </item>
</layer-list>
